{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ],
  "functions": {
    "server.js": {
      "maxDuration": 60,
      "includeFiles": [
        "dist/**",
        "package.json",
        "node_modules/@strapi/**/package.json",
        "node_modules/@strapi/**/schema.json"
      ]
    }
  }
}