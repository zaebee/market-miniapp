{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 60,
      "includeFiles": "{.cache/**,dist/**,package.json,node_modules/@strapi/**/**,node_modules/pluralize/**,node_modules/koa-range/**,database/**,public/**}"
    }
  }
}