{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 60,
      "includeFiles": "{dist/**,package.json,node_modules/@strapi/**/package.json,node_modules/@strapi/**/schema.json,database/**,public/**}"
    }
  }
}