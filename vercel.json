{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 60,
      "includeFiles": "{.cache/**,dist/**,package.json,node_modules/@strapi/**,database/**,public/**}",
      "excludeFiles": "{**/*.map,**/*.d.ts,node_modules/**/*.map,node_modules/**/*.d.ts}"
    }
  }
}