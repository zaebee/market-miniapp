{
  "kind": "collectionType",
  "collectionName": "apartments",
  "info": {
    "singularName": "apartment",
    "pluralName": "apartments",
    "displayName": "Apartment",
    "description": "Apartment listings"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "address": {
      "type": "string"
    },
    "bedrooms": {
      "type": "integer",
      "min": 0
    },
    "bathrooms": {
      "type": "integer",
      "min": 0
    },
    "area": {
      "type": "decimal",
      "min": 0
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images"]
    },
    "agent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::agent.agent",
      "inversedBy": "apartments"
    },
    "city": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::city.city",
      "inversedBy": "apartments"
    }
  }
}
